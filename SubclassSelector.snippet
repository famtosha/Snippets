<?xml version="1.0" encoding="utf-8"?>
<CodeSnippets xmlns="http://schemas.microsoft.com/VisualStudio/2005/CodeSnippet">
    <CodeSnippet Format="1.0.0">
        <Header>
            <Title>SubclassSelector</Title>
		 <Author>Famtosha</Author>
            <Description>Subclass selector for unity</Description>
            <Shortcut>ss</Shortcut>
        </Header>
        <Snippet>
            <Code Language="CSharp">
                <![CDATA[[SerializeReference, SubclassSelector] private $Type$ $Name$;$end$]]>
            </Code>
			<Declarations>
				<Literal>
					<ID>Type</ID>
					<ToolTip>Field type</ToolTip>
					<Default>int</Default>
				</Literal>
				<Literal>
					<ID>Name</ID>
					<ToolTip>Field name</ToolTip>
					<Default>value</Default>
				</Literal>
			</Declarations>
			<Imports>
				<Import>
					<Namespace>UnityEngine</Namespace>
				</Import>
			</Imports>
        </Snippet>
    </CodeSnippet>
</CodeSnippets>